                  </Label>
                  <Line endX="350.0" startX="1.0">
                     <VBox.margin>
                        <Insets bottom="30.0" />
                     </VBox.margin>
                  </Line>
                  <TableView fx:id="pacTable" prefHeight="200.0" prefWidth="449.0">
                     <columns>
                        <TableColumn fx:id="cpfCol" minWidth="0.0" prefWidth="145.0" text="CPF" />
                        <TableColumn fx:id="pacCol" prefWidth="303.0" text="Nome" />
