                     <VBox.margin>
                        <Insets top="30.0" />
                     </VBox.margin>
                  </Label>
                  <Line endX="350.0" startX="1.0">
                     <VBox.margin>
                        <Insets bottom="30.0" />
                     </VBox.margin>
                  </Line>
                  <DatePicker fx:id="datePicker" />
                  <HBox prefHeight="37.0" prefWidth="445.0">
                     <children>
                        <Button mnemonicParsing="false" onAction="#updateData" prefHeight="37.0" prefWidth="150.0" styleClass="primary_blue" text="Salvar">
